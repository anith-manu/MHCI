<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anithmanu.universitytour">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LeaderboardActivity"></activity>
        <activity android:name=".MapActivity" />
        <activity android:name=".GroupCreationActivity" />
        <activity android:name=".SelectGroupActivity" />
        <activity android:name=".HuntingActivity" />
        <activity android:name=".GroupActivity" />
        <activity android:name=".SingleActivity" />
        <activity android:name=".GuidedTourActivity" />
        <activity android:name=".FutureActivity" />
        <activity
            android:name=".ModesActivity"
            android:label="@string/title_activity_modes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ModesMenu" />
        <activity android:name=".ExploreBuildings" />
        <activity android:name=".LogInActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>